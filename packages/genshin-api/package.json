{
  "name": "@mihoyo-kit/genshin-api",
  "version": "0.1.0",
  "license": "MIT",
  "description": "Provides some encapsulated utilities function for Genshin Impact.",
  "homepage": "https://github.com/leonismoe/genshin-stats/tree/main/packages/genshin-api",
  "repository": "github:leonismoe/genshin-stats",
  "sideEffects": false,
  "type": "module",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./typings": "./typings.d.ts",
    "./lib/*": {
      "import": "./lib/*.mjs",
      "require": "./lib/*.js",
      "default": "./lib/*.js"
    }
  },
  "files": [
    "*.d.ts",
    "*.json",
    "lib",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "npx tsc -m es2020 && node post-build.js mjs",
    "build:cjs": "npx tsc -m commonjs -d && node post-build.js",
    "clean": "rm -rf lib",
    "ts-node": "node --loader ts-node/esm --conditions=ts-node --experimental-specifier-resolution=node"
  },
  "peerDependencies": {
    "@mihoyo-kit/api": "^0.1.0",
    "@mihoyo-kit/genshin-data": "^2.1.0",
    "undici": "^4.5.1"
  },
  "devDependencies": {
    "typescript": "^4.4.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
